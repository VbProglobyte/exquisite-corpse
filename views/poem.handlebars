<!--Page where user will view a single finished poem.-->

<div class='row'>
  <div class='col-12 d-flex justify-content-center text-center'>
    <h1>EXQUISITE CORPSE TESTING: THIS IS WHERE ALL THE FRAGMENTS WILL APPEAR
      SOMEWHERE</h1>
  </div>
</div>

{{#each fragments as |fragment|}}
  <div class='row'>
    <div class='col-12 d-flex justify-content-center text-center'>
      {{fragment.text_input}}
      {{user.username}}
      <br />
    </div>
  </div>
{{/each}}